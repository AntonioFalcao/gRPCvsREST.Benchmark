{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "AllowedHosts": "*",
  
  "Kestrel": {
    "EndpointDefaults": {
      "ClientCertificateMode": "AllowCertificate",
      "SslProtocols": ["Tls11", "Tls12", "Tls13"],
      "Protocols": "Http2"
    }
  }
}
